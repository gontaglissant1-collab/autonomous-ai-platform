{
  "build": {
    "env": {
      "PYTHON_VERSION": "3.11"
    },
    "command": "python3.11 main.py"
  },
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.11"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend_build/$1"
    }
  ]
}
